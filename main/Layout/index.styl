.homePage
  display flex
  align-items center
  background-image  url(img/background.png)
  background-repeat no-repeat
  background-color #e6e6e6
  width 100%

.header-menu
  display flex
  flex-direction row
  align-items center
  width 80%
  justify-content space-between

.header-logo
    background-image url(img/logo.png)
    height 150px
    outline-color rgb(41, 98, 255)
    width 150px
    color green
    background-position center
    background-repeat no-repeat
    background-size contain
    margin-right 30px

.menu-item
  align-items center
  text-transform uppercase

.body
  width 80%

.Item
  color white
  outline-color white








